irejs:\n\n~~~javascript\ndefine(['path/url-join.js'], function (urljoin) {\n  urljoin('http://blabla.com', 'foo?a=1');\n});\n~~~\n\n## License\n\nMIT\n","maintainers":[{"name":"jfromaniello","email":"jfromaniello@gmail.com"}],"time":{"modified":"2016-10-03T14:12:45.693Z","created":"2013-01-29T15:47:51.977Z","0.0.1":"2013-01-29T15:47:55.916Z","1.0.0":"2016-03-23T13:07:31.637Z","1.1.0":"2016-04-05T11:47:48.154Z"},"author":{"name":"José F. Romaniello","email":"jfromaniello@gmail.com","url":"http://joseoncode.com"},"repository":{"type":"git","url":"git://github.com/jfromaniello/url-join.git"},"users":{"passcod":true,"gztomas":true,"joeyespo":true,"xiechao06":true,"docksteaderluke":true,"bojand":true,"jybleau":true,"scottfreecode":true},"homepage":"https://github.com/jfromaniello/url-join#readme","keywords":["url","join"],"bugs":{"url":"https://github.com/jfromaniello/url-join/issues"},"license":"MIT","readmeFilename":"README.md","_attachments":{},"_etag":"\"1L52R4EQ02VEUC59QNS5FB8B6\""}